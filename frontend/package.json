{
  "name": "cashflow-frontend",
  "version": "1.4.3",
  "build_date": "2024-01-15",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test tests/e2e/ui-fixes.spec.ts",
    "test:e2e:stories": "playwright test tests/e2e/story-*.spec.ts",
    "test:e2e:all": "playwright test tests/e2e/",
    "test:report": "playwright show-report",
    "test:install": "playwright install --with-deps chromium firefox",
    "clean": "find src -name '*.js' -type f -delete && echo 'âœ… Cleaned compiled JavaScript files'",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^21.1.1",
    "playwright": "^1.54.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^3.2.4"
  }
}
